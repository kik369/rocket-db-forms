{% extends "base" %} {% block content %}
<hgroup>
    <h2>all users</h2>
    <p>only visible to admins</p>
</hgroup>
{% for entry in all_users %}
<p>
    id: {{ entry.id }}, email: {{ entry.email }}
    {% if entry.admin %}
    <b><ins>admin</ins></b>
    {% else %}
    <b>{{ entry.admin }}</b>
    {% endif %}<br />
    hashed password: {{ entry.password }}
</p>
{% endfor %}{% endblock %}
